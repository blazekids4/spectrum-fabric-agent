{
  "buildCommand": "cd frontend && pnpm run build",
  "outputDirectory": "frontend/.next",
  "devCommand": "cd frontend && pnpm run dev",
  "installCommand": "cd frontend && pnpm install",
  "functions": {
    "api/index.py": {
      "runtime": "python@3.12"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    }
  ]
}